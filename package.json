{
  "name": "qie-agora-umi",
  "version": "0.0.1",
  "description": "基于agora的多人互动直播项目",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "authors": {
    "name": "inter",
    "email": ""
  },
  "repository": "/qie-agora-umi",
  "scripts": {
    "dev": "father doc dev",
    "build": "father build",
    "lerna": "lerna",
    "lint-staged:ts": "eslint --ext .js,.jsx,.ts,.tsx --format=pretty"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,less,md,json}": [
      "prettier --write"
    ],
    "**/*.{js,jsx}": "yarn run lint-staged:ts",
    "**/*.{js,ts,tsx}": "yarn run lint-staged:ts",
    "yarn.lock": [
      "git rm --cached"
    ]
  },
  "peerDependencies": {
    "react": "16.x"
  },
  "devDependencies": {
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/agora-rtc-sdk": "^3.0.0",
    "@typescript-eslint/experimental-utils": "^2.21.0",
    "@typescript-eslint/typescript-estree": "^2.21.0",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "eslint-plugin-react-hooks": "^1.6.1",
    "father": "^2.16.0",
    "husky": "^3.0.9",
    "lerna": "^3.20.2",
    "lint-staged": "^10.0.8",
    "stylelint-config-recommended": "^2.2.0",
    "stylelint-config-standard": "^18.3.0",
    "typescript": "^3.7.2",
    "umi": "^2.13.3"
  },
  "license": "MIT",
  "dependencies": {
    "agora-rtc-sdk": "^3.0.1",
    "pubsub-js": "^1.8.0"
  },
  "resolutions": {
    "docz-core/typescript": "^3.7.2"
  }
}
